{
  "name": "@sed/scripts",
  "version": "0.1.0",
  "private": true,
  "description": "SED development and build scripts",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/dev.ts",
    "build:all": "tsx src/build.ts",
    "clean": "tsx src/clean.ts",
    "release": "tsx src/release.ts",
    "check": "tsx src/check.ts",
    "sync-versions": "tsx src/sync-versions.ts",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "execa": "^8.0.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "ora": "^8.0.1",
    "prompts": "^2.4.2",
    "semver": "^7.7.4"
  },
  "devDependencies": {
    "@sed/config": "workspace:*",
    "@types/fs-extra": "^11.0.4",
    "@types/prompts": "^2.4.9",
    "@types/semver": "^7.5.6",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}

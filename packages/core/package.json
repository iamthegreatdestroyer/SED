{
  "name": "@sed/core",
  "version": "0.0.0",
  "description": "Core Semantic Entropy Differencing engine",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./semantic": {
      "types": "./dist/semantic/index.d.ts",
      "import": "./dist/semantic/index.js"
    },
    "./entropy": {
      "types": "./dist/entropy/index.d.ts",
      "import": "./dist/entropy/index.js"
    },
    "./engine": {
      "types": "./dist/engine/index.d.ts",
      "import": "./dist/engine/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist coverage"
  },
  "dependencies": {
    "@sed/shared": "workspace:*",
    "tree-sitter": "^0.21.0",
    "tree-sitter-c": "^0.21.0",
    "tree-sitter-cpp": "^0.21.0",
    "tree-sitter-go": "^0.21.0",
    "tree-sitter-java": "^0.21.0",
    "tree-sitter-javascript": "^0.25.0",
    "tree-sitter-python": "^0.21.0",
    "tree-sitter-rust": "^0.21.0",
    "tree-sitter-typescript": "^0.21.0"
  },
  "devDependencies": {
    "@sed/config": "workspace:*",
    "@types/node": "^20.11.0",
    "@vitest/coverage-v8": "^1.2.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "onnxruntime-node": "^1.16.0"
  },
  "peerDependenciesMeta": {
    "onnxruntime-node": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "license": "MIT"
}
